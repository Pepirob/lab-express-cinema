<div>
  <a href="/movies">Go back</a>
  <h3>{{this.title}}</h3>
  <h4>Director: {{this.director}}</h4>
  <ul>
    <h4>Stars</h4>
    {{#each this.stars}}
      <li>{{this}}</li>
    {{/each}}
    <p class="description">{{this.description}}</p>

    <p>
      {{#each this.showtimes}}
        <span> {{this}} |</span>
      {{/each}}
    </p>
  </ul>
  
</div>